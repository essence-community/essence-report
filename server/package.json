        "body-parser": "^1.19.0",
        "bwip-js": "^3.0.4",
        "connect": "^3.2.0",
        "cors": "^2.8.5",
        "cron": "^1.8.2",
        "cron-parser": "^4.2.0",
        "dotenv": "^10.0.0",
        "etag": "^1.8.1",
        "express": "^4.20.0",
        "handlebars": "^4.7.7",
        "handlebars-helpers": "^0.10.0",
        "html2json": "^1.0.2",
        "is-utf8": "^0.2.1",
        "js-string-escape": "^1.0.1",
        "js-yaml": "^4.0.0",
        "@jsreport/jsreport-core": "^4.5.0",
        "@jsreport/jsreport-handlebars": "^4.0.1",
