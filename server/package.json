        "@types/jsreport-handlebars": "^2.1.0",
        "@types/mime": "^2.0.3",
        "@types/minimatch": "^3.0.4",
        "@types/node": "^16.11.11",
        "@types/uuidv4": "^5.0.0",
        "cross-env": "^7.0.3",
        "nyc": "^15.1.0",
        "rimraf": "^3.0.2",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "typescript": "^4.2.4"
