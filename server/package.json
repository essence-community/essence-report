        "marked": "^4.0.6",
        "mime": "^3.0.0",
        "minimatch": "^3.0.4",
        "nodemon": "^2.0.7",
        "rufus": "^0.5.7",
        "typescript-ioc": "^3.2.2",
        "typescript-rest": "^3.0.2",
        "typescript-rest-ioc": "^1.0.0",
        "typescript-rest-swagger": "^1.1.6",
        "uuidv4": "^6.2.7",
        "xmldom": "^0.6.0",
        "xpath": "^0.0.32"
    },
    "devDependencies": {
        "@codegena/oapi3ts": "^3.0.0-alpha.3",
        "@codegena/oapi3ts-cli": "^2.1.10-alpha.8",
        "@essence-report/plugininf": "^1.0.0",
