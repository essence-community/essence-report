{
    "name": "@essence-report/server",
    "version": "1.0.0",
    "description": "Essence Report Server",
    "author": "Ivan Artemov (honyrik@gmail.ru)",
    "license": "MIT",
    "main": "src/index.js",
    "scripts": {
        "tsc": "tsc",
        "start": "./node_modules/.bin/ts-node src/index.ts",
        "start:in:docker": "node dist/index.js",
        "start:cluster": "./node_modules/.bin/ts-node src/cluster.ts",
        "build": "npm run clean && gulp server",
        "clean": "rimraf dist",
        "pretest": "cross-env NODE_ENV=test npm run build && npm run lint",
        "test": "cross-env NODE_ENV=test mocha --exit",
        "test:coverage": "nyc npm run test",
        "generate:typing": "node ./update-typings.js --srcPath ./spec/openapi.json --destPath ./src --separatedFiles true",
        "swagger": "swaggerGen -c ./swagger.config.yml"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.12.0",
        "adm-zip": "^0.5.5",
